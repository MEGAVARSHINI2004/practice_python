#scoreboard
for _ in range(int(input())):
    name = input()
    score = float(input())
    d=dict(zip(name,score))
    l=list(map(list,d.items()))
    print(l)

#Example1
n = int(input())
arr = map(int, input().split())
if(2 <= n <= 10):
    if(-100 <= arr[n] <= 100):
        m=max(arr)
        print(m)
        